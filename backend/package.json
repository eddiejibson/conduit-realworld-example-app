{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "node --watch index.js",
    "sqlz": "npx sequelize-cli",
    "typeorm": "typeorm",
    "migration:generate": "typeorm migration:generate -d ./db.config.js -o",
